{
  "name": "gittyup",
  "version": "0.1.0",
  "description": "Multi-repo orchestration CLI with interactive conflict resolution",
  "type": "module",
  "bin": {
    "gittyup": "./bin/gittyup.js"
  },
  "main": "./dist/cli.js",
  "files": [
    "dist/",
    "bin/"
  ],
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit/",
    "test:integration": "vitest run tests/integration/",
    "test:e2e": "vitest run tests/e2e/"
  },
  "keywords": [
    "git",
    "multi-repo",
    "cherry-pick",
    "merge",
    "conflict-resolution",
    "cli"
  ],
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.3",
    "js-yaml": "^4.1.1",
    "octokit": "^4.0.2",
    "ora": "^8.1.0",
    "simple-git": "^3.27.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.2.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
